	<queryString>
		<![CDATA[SELECT u.*, p.nip, p.nama,
concat(lpad(day(p.tmt_pangkat), 2, 0), '-', lpad(month(p.tmt_pangkat), 2, 0), '-', year(p.tmt_pangkat)) tmt_parsed
